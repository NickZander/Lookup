<div class="text-center" style="padding-top: 18%;font-family: Alfa Slab One;vertical-align: middle;">
  <p>Find a Eat-out</p>
  <%= form_for(@business)  do |f| %>
    <%= f.text_field :term, class: "responsive-input", :style => "border-radius:2px;" %>
    <%= f.submit "Look up", class: "btn btn-outline-secondary responsive-button responsive-search", :style => "margin-left: -5.5px; border-radius:2px;"%>
  <% end %>
</div>


